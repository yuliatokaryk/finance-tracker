<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">Ticker</th>
      <th scope="col">Name</th>
      <th scope="col">Last price</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @stocks.each do |stock|%>
      <tr>
        <th scope="row"><%= stock.ticker %></th>
        <td><%= stock.name %></td>
        <td><%= stock.last_price %></td>
        <td>@mdo</td>
      </tr>
    <% end %>
  </tbody>
</table>
